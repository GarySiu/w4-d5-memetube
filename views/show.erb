<% @videos.each do |video| %>
<%= "<p>#{video['title']} [<a href=\"/videos/#{video['id']}/edit\">edit</a>] [<a href=\"/videos/#{video['id']}/delete\">delete</a>]</p>"%>
<%= "<iframe type=\"text/html\" width=\"640\" height=\"390\" src=\"http://www.youtube.com/embed/#{video['video_id']}?autoplay=0\" frameborder=\"0\"/></iframe>" %>
<%= "<p>#{video['description']}</p>" %>
<% end %>